<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.malin.plugin.contentprovider">

    <application tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".PluginContentProviderActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <provider
            android:name="com.malin.plugin.contentprovider.PluginContentProvider"
            android:authorities="com.malin.plugin.contentprovider.PluginContentProvider"
            android:exported="true" />
    </application>
</manifest>